{
  "name": "ts-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
    "storybook": "start-storybook -p 6006",
    "eject": "react-scripts-ts eject",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts-ts": "^3.1.0",
    "styled-components": "^3.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-actions": "^3.3.4",
    "@storybook/addon-info": "^3.3.4",
    "@storybook/addon-knobs": "^3.3.4",
    "@storybook/addon-links": "^3.3.4",
    "@storybook/addon-notes": "^3.3.4",
    "@storybook/addon-options": "^3.3.4",
    "@storybook/addons": "^3.3.4",
    "@types/enzyme": "3.1.13",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/expect": "1.20.3",
    "@storybook/react": "^3.3.4",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-runtime": "^6.26.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "expect": "1.20.2",
    "jest": "^22.4.4",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "storybook-addon-specifications": "^2.1.2",
    "ts-jest": "^22.4.6",
    "typescript": "^3.0.0"
  }
}
